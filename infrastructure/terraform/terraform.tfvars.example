# =============================================================================
# Career Agent - Terraform Variables Example
# =============================================================================
# Copy this to dev.tfvars or prod.tfvars and fill in your values
# DO NOT COMMIT the filled-in tfvars file to version control

# =============================================================================
# Project Configuration
# =============================================================================

environment  = "dev"  # or "prod"
project_name = "career-agent"

# =============================================================================
# Railway
# =============================================================================

# Get your Railway API token from: https://railway.app/account/tokens
railway_api_token = ""

# Database credentials (will be used for PostgreSQL service)
postgres_db       = "career_agent"
postgres_user     = "career_agent"
# postgres_password = ""  # Optional: auto-generated if not set

# =============================================================================
# Vercel
# =============================================================================

# Get your Vercel API token from: https://vercel.com/account/tokens
vercel_api_token = ""

# Optional: Vercel team ID (only if using a team account)
vercel_team_id = null

# =============================================================================
# Application Secrets
# =============================================================================

# Get your Anthropic API key from: https://console.anthropic.com/
anthropic_api_key = ""

# jwt_secret = ""  # Optional: auto-generated if not set

# =============================================================================
# GitHub Repository
# =============================================================================

github_repo   = "denisenanni/career-agent"
github_branch = "main"
